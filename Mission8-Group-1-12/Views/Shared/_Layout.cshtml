

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />

    <!--Dynamically calls variable called title referenced in the razor of all Views pages to determine title per page -->
    <title>@ViewData["Title"] - Mission 8</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link rel="icon" href="https://www.shutterstock.com/shutterstock/photos/1147101026/display_1500/stock-vector-outstanding-professional-elegant-trendy-awesome-artistic-black-and-white-color-tj-jt-initial-based-1147101026.jpg" /> <!--icon for tab-->
</head>
<body>
    <header>
        <!--Create a header-->
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <ul class="navbar-nav flex-grow-1">
                <li class="nav-item">
                    <a class="navbar-brand" asp-controller="Home" asp-action="Index">Home</a>
                </li>
                <li class="nav-item">
                    <a class="navbar-brand" asp-controller="Home" asp-action="Quadrant">Quadrants</a>
                </li>
                <li class="nav-item">
                    <a class="navbar-brand" asp-controller="Home" asp-action="ToDoList">To-Do List</a>
                </li>
                <li class="nav-item">
                    <a class="navbar-brand" asp-controller="Home" asp-action="listEntry">Enter Task</a>
                </li>
            </ul>
        </nav>
    </header>
    <div>
        @RenderBody()
    </div>


        <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
</body>

</html>
